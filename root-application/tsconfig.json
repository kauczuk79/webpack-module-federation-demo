{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ES2020",
    "target": "es5",
    "jsx": "react",
    "noCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "paths": {
      "*": ["./@mf-types/*"]
    }
  }
}
